<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         point="org.eclipse.core.runtime.products">
      <product
            name="clonemap"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="applicationCSS"
               value="platform:/plugin/clonemap/css/default.css">
         </property>
      </product>
   </extension>
   <extension
         id="simpleZestProject4.fragment"
         point="org.eclipse.e4.workbench.model">
      <fragment
            uri="fragment.e4xmi">
      </fragment>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
         <perspective
            class="perspective.CloneMapPerspective"
            id="perspective.CloneMapPerspective"
            name="CloneMap Perspective"/>
   </extension>
<extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="perspective.CloneMapPerspective">
         
      </perspectiveExtension>
   </extension>
</plugin>
